  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20",
    "@typescript-eslint/parser": "^8.29.1",
    "eslint": "^9.24.0",
    "globals": "^16.0.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.1",
    "unbuild": "^3.5.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vfshera/local-s3.git"
  },
  "bugs": {
    "url": "https://github.com/vfshera/local-s3/issues"
