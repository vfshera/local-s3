{
  "name": "local-s3",
  "version": "0.0.1",
  "description": "A lightweight S3-like object storage system for local filesystem in Node.js",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
